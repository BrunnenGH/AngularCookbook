{
  "version": 1,
  "projects": {
    "guide": {
      "projectType": "application",
      "root": "apps/guide",
      "sourceRoot": "apps/guide/src",
      "prefix": "nx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/guide",
            "index": "apps/guide/src/index.html",
            "main": "apps/guide/src/main.ts",
            "polyfills": "apps/guide/src/polyfills.ts",
            "tsConfig": "apps/guide/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/guide/src/favicon.ico", "apps/guide/src/assets"],
            "styles": ["apps/guide/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/guide/src/environments/environment.ts",
                  "with": "apps/guide/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "baseUrl": "http://localhost:4201/",
              "baseHref": "/cb/",
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [{
                "replace": "configuration/config.serve.json",
                "with": "configuration/config.development.serve.json"
              }]
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "guide:build:production"
            },
            "development": {
              "browserTarget": "guide:build:development",
              "proxyConfig": "configuration/proxy.development.js"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "guide:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/guide/src/**/*.ts",
              "apps/guide/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/guide"],
          "options": {
            "jestConfig": "apps/guide/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "guide-e2e": {
      "root": "apps/guide-e2e",
      "sourceRoot": "apps/guide-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/guide-e2e/cypress.json",
            "devServerTarget": "guide:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "guide:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/guide-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["guide"]
    }
  }
}
